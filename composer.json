    "vlucas/phpdotenv": "5.4.1",
    "ext-pdo": "*",
    "symfony/apache-pack": "^1.0",
    "ext-json": "*",
    "guzzlehttp/guzzle": "^7.4",
    "ext-http": "*",
    "symfony/console": "6.0.9",
    "google/apiclient-services": "v0.254.0",
    "google/apiclient": "2.12.6",
    "symfony/flex": "2.1.1",
    "psr/container": "2.0.2",
